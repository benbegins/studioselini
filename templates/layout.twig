<!doctype html>
<!--[if lt IE 9]><html class="no-js no-svg ie lt-ie9 lt-ie8 lt-ie7" {{ site.language_attributes }}> <![endif]-->
<!--[if IE 9]><html class="no-js no-svg ie ie9 lt-ie9 lt-ie8" {{ site.language_attributes }}> <![endif]-->
<!--[if gt IE 9]><!--><html class="no-js no-svg scroll-smooth" {{ site.language_attributes }}> <!--<![endif]-->
<head>
{% block html_head_container %}
    {% include 'partials/head.twig' %}
    {% block head %}
    {% endblock %}
{% endblock %}
</head>

<body class="{{body_class}}" data-template="layout.twig">
    {{function('wp_body_open')}}

    {% include "header.twig" %}

    <div v-scope="Intro()" class="pointer-events-none" @vue:mounted="init($el)">
        <svg class="transition fixed z-50 inset-0 w-full h-full pointer-events-none" viewBox="0 0 100 100" preserveAspectRatio="none">
            <path class="path fill-white" vector-effect="non-scaling-stroke" d="M 0 0 V 100 Q 50 100 100 100 V 0 z" />
        </svg>
        <div class="transition-overlay fixed z-40 w-full h-full inset-0 bg-black pointer-events-none opacity-0"></div>
    </div>

    <main id="content" role="main" class="pt-40 relative overflow-clip">
        {% block content %}
            Sorry, no content
        {% endblock %}
    </main>

    {% include 'footer.twig' %}

    {{ function('wp_footer') }}
</body>
</html>
